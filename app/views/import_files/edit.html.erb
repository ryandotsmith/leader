<div id="left">
	
</div><!--end left-->
<div id="center">
	<h1>Editing import_file</h1>

	<% form_for(@import_file) do |f| %>
	  <%= f.error_messages %>

	  <p>
	    <%= f.label :comment %><br />
	    <%= f.text_field :comment %>
	  </p>

		<p>
			<% unless @import_file.consumed %>
				<%= f.label "check box to import csv" %>
				<%= f.check_box :update_hook_param %>
			<% end %>
		</p>

	  <p>
	    <%= f.submit "Update" %>
	  </p>

	<% end %>

	<%= link_to 'Show', @import_file %> |
	<%= link_to 'Back', import_files_path %>
	
</div><!--end center-->
