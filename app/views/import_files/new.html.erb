<h1>New import_file</h1>

<% form_for(@import_file , :html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_field :comment %>
  </p>
	<p>
		<%= f.file_field :lead_data %>
	</p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', import_files_path %>
