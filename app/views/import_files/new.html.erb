<div id="left">
	
</div><!--end left-->
<div id="center">
	<h1>New import file</h1>
	<h2> 
		<p>Once a file has been uploaded, you must import it into the leads table.<br />
		   To import a file into the leads table. Edit the import file and 
			 check the 'import' box.
		</p>
	</h2>
	<% form_for(@import_file , :html => { :multipart => true }) do |f| %>
	  <%= f.error_messages %>
	  <p>
	    <%= f.label :comment %><br />
	    <%= f.text_field :comment %>
	  </p>
		<p>
			<%= f.file_field :lead_data %>
		</p>
	  <p>
	    <%= f.submit "Create" %>
	  </p>
	<% end %>

	<%= link_to 'Back', import_files_path %>
	
</div><!--end center-->
